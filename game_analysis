    "fig = go.Figure()\n",
    "\n",
    "for genre in genre_distribution.columns[:-1]:\n",
    "    genre_data = relevant_slice[relevant_slice['genre'] == genre]\n",
    "    pivot = genre_data.pivot_table(\n",
    "        index='year_of_release', values='total_sales', aggfunc='sum')\n",
    "\n",
    "    fig.add_trace(go.Scatter(x=pivot.index, y=pivot.total_sales, name=genre))\n",
    "    fig.update_layout(title_text='Sales by genre 2013-2016', \n",
    "                  yaxis_title_text='total sales',\n",
    "                  xaxis_title_text='years',\n",
    "                     )\n",
    "fig.show()\n",
    "    "
