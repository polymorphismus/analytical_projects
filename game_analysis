   "cell_type": "code",
   "execution_count": 130,
   "metadata": {},
   "outputs": [],
   "source": [
    "def scatter_plot(database, x_plot, y_plot, Q3, max_score, median_line):\n",
    "    fig = px.scatter(database, \n",
    "                 x=x_plot, \n",
    "                 y=y_plot, \n",
    "                 trendline=\"ols\")\n",
    "    \n",
    "    fig.add_shape(type='line',\n",
    "                x0=0,\n",
    "                y0=Q3,\n",
    "                x1=max_score,\n",
    "                y1=Q3,\n",
    "                line=dict(color='LightSeaGreen'),\n",
    "                name='Q3'              \n",
    ")\n",
    "    fig.add_shape(type='line',\n",
    "                x0=0,\n",
    "                y0=median_line,\n",
    "                x1=max_score,\n",
    "                y1=median_line,\n",
    "                line=dict(color='MediumPurple'),\n",
    "                name='median_line'              \n",
    ")\n",
    "    fig.update_layout(title_text=x_plot + ' and game sales corellation on ' + database.platform.mode().astype('string')[0],\n",
    "                 xaxis_title_text= x_plot, \n",
    "                  yaxis_title_text=y_plot)\n",
    "    \n",
    "    fig.show()\n",
    "    \n",
    "    "
   ]
  },
