   "cell_type": "code",
   "execution_count": 104,
   "metadata": {},
   "outputs": [],
   "source": [
    "games[['user_score'] + sale_names] = games[['user_score'] + sale_names].astype('float32')\n",
    "games[['year_of_release', 'critic_score']] = games[[\n",
    "    'year_of_release', 'critic_score']].astype('int')"
   ]
